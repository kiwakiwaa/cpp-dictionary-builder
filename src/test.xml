            <SubItemG id="00055-C021">
                <SubItem id="00055-8001">
                    <SubItemC>
                        <SubItemHeadG>
                            <headword class="熟語"><SubItemP class="S">【</SubItemP>乗雲<SubItemP
                                    class="E">】</SubItemP></headword>
                            <yomi>ジョウウン</yomi>
                        </SubItemHeadG>
                        <MG id="00055-C022" class="inline">
                            <span id="00055-F011"><Wc class="Logo-FM">①</Wc>雲に乗る。</span>
                        </MG>
                        <MG id="00055-C023" class="inline">
                            <span id="00055-F012"><Wc class="Logo-FM">②</Wc>高位にのぼる。</span>
                        </MG>
                    </SubItemC>
                </SubItem>
                <SubItem id="00055-8002">
                    <SubItemC>
                        <SubItemHeadG>
                            <headword class="熟語"><SubItemP class="S">【</SubItemP>乗鶴<SubItemP
                                    class="E">】</SubItemP></headword>
                            <yomi>ジョウカク</yomi>
                        </SubItemHeadG>
                        <MG id="00055-C024" class="inline">
                            <span id="00055-F013"><Wc class="Logo-FM">①</Wc>鶴<mlg>つる</mlg>に乗って天に上る。</span>
                        </MG>
                        <MG id="00055-C025" class="inline">
                            <span id="00055-F014"><Wc class="Logo-FM">②</Wc>仙人になる。</span>
                        </MG>
                    </SubItemC>
                </SubItem>
                <SubItem id="00055-8003">
                    <SubItemC>
                        <SubItemHeadG>
                            <headword class="熟語"><SubItemP class="S">【</SubItemP>乗隙<SubItemP
                                    class="E">】</SubItemP></headword>
                            <yomi>ジョウゲキ</yomi>
                            <YomiMDot>・</YomiMDot>
                            <yomi>すきに ジョウず</yomi>
                        </SubItemHeadG>
                        <MG class="inline">
                            <span id="00055-F015">すきにつけいる。乗間。</span>
                        </MG>
                    </SubItemC>
                </SubItem>
                <SubItem id="00055-8004">
                    <SubItemC>
                        <SubItemHeadG>
                            <headword class="熟語"><SubItemP class="S">【</SubItemP>乗軒<SubItemP
                                    class="E">】</SubItemP></headword>
                            <yomi>ジョウケン</yomi>
                        </SubItemHeadG>
                        <MG class="inline">
                            <span id="00055-F016">大夫<mlg>タイフ</mlg>の車に乗る。大夫となること。軒は、大夫の車の意。</span>
                        </MG>
                    </SubItemC>
                </SubItem>
                <SubItem id="00055-8005">
                    <SubItemC>
                        <SubItemHeadG>
                            <headword class="熟語"><SubItemP class="S">【</SubItemP>乗矢<SubItemP
                                    class="E">】</SubItemP></headword>
                            <yomi>ジョウ シ</yomi>
                        </SubItemHeadG>
                        <MG class="inline">
                            <span id="00055-F017">一そろいの矢。矢は、四本を一そろいとする。<Source>〈孟子・離婁〉</Source></span>
                        </MG>
